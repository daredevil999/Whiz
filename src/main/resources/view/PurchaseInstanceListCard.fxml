<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="35.0" prefWidth="244.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane prefHeight="251.0" prefWidth="242.0" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="2.5" prefWidth="3.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="4.0" minWidth="2.5" prefWidth="3.0" />
            </columnConstraints>
            <children>
            	<Label fx:id="index" prefHeight="3.0" prefWidth="3.0" styleClass="cell_small_index_label" text="\$index">
                    <font>
                        <Font size="10.0" />
                    </font>
                </Label>
                <VBox alignment="CENTER_LEFT" GridPane.columnIndex="0" VBox.vgrow="ALWAYS">
                    <stylesheets>
                        <URL value="@DarkTheme.css" />
                        <URL value="@Extensions.css" />
                    </stylesheets>
                    <padding>
                        <Insets bottom="10" left="20" right="10" top="10" />
                    </padding>

                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="12.0" prefWidth="216.0" spacing="5">
                            <children>
                                <Label fx:id="date" prefHeight="12.0" prefWidth="55.0" styleClass="cell_small_list" text="\\$date" wrapText="true">
                                    <font>
                                        <Font size="10.0" />
                                    </font>
                                </Label>
                                <Label fx:id="price" prefHeight="12.0" prefWidth="55.0" styleClass="cell_small_list" text="\\$price" wrapText="true">
                                    <font>
                                        <Font size="10.0" />
                                    </font>
                                </Label>
                                <Label fx:id="lots" prefHeight="12.0" prefWidth="55.0" styleClass="cell_small_list" text="\\$lots" wrapText="true">
                                    <font>
                                        <Font size="10.0" />
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </GridPane>
    </children>
</HBox>
